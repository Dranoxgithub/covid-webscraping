        <a href="#" onclick="showFeedbackDialog();" id="btnFeedback" class="btnFeedback">Contact Us</a>
        
        
        <div id="atlwdg-blanket" class="atlwdg-blanket" style="display: none;"></div>
        <div id="atlwdg-container" class="atlwdg-popup">
            <div id="atlScriptlet">
                <div class="aui-dialog collector-dialog">

                    <h2 class="dialog-title">How can we help?</h2>

                    
                    <div class="content-body">
                        <div class="msg-container"></div>

                        <div id="feedbackForm">
                            <div class="aui-message info custom-msg"><span class="aui-icon icon-info"></span>
                                <p style="margin:0;">Please provide as much detail below as possible so City staff can respond to your inquiry:</p>
                            </div>
							<div class="desc-group field-group">
								<label>Reason<span class="required">*</span></label>
								<div class="buttonList" style="display:inline-block;">
									<div>
										<input type="radio" id="feedbackType1" name="feedbackType" value="General Question/Comment" checked required />
										<label for="feedbackType1">General Question/Comment</label>
									</div>
									<div>
										<input type="radio" id="feedbackType2" name="feedbackType" value="COVID-19 Question" />
										<label for="feedbackType2">COVID-19 Question</label>
									</div>
									<div>
										<input type="radio" id="feedbackType3" name="feedbackType" value="Website Issue/Feedback" />
										<label for="feedbackType3">Website Issue/Feedback</label>
									</div>
								</div>
							</div>
							<div class="desc-group field-group">
                                <label for="feedback">Description:<span class="required">*</span></label>
                                <textarea class="textarea long-field" cols="30" rows="5" id="feedback" name="feedback"></textarea>
                            </div>
                            <div class="desc-group field-group">
                                <label for="feedback_name">Name:</label>
                                <input id="feedback_name" type="text" />
                            </div>
                            <div class="desc-group field-group">
                                <label for="feedback_email">Email:</label>
                                <input id="feedback_email" type="email" />
                            </div>
